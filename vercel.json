{
  "redirects": [
    {
      "source": "/:path((?!en/).*)",
      "destination": "/en/:path*",
      "permanent": true
    },
    {
      "source": "/:path((?!zh/).*)",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "zh"
        }
      ],
      "destination": "/zh/:path*",
      "permanent": false
    }
  ]
}
