{
  "rewrites": [
    {
      "source": "/:path((?!en-US/).*)",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "en"
        }
      ],
      "destination": "/en-US/:path*"
    },
    {
      "source": "/:path((?!zh-Hans/).*)",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "zh"
        }
      ],
      "destination": "/zh-Hans/:path*"
    }
  ]
}
